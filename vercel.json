{
  "version": 2,
  "builds": [
    { "src": "api/handler.js", "use": "@vercel/node" },
    { "src": "pages/index.js", "use": "@vercel/next" }, 
    { "src": "package.json", "use": "@vercel/next" }
  ],
  "routes": [
    { "src": "/api/handler", "dest": "/api/handler.js" },
    { "src": "/", "dest": "/index.html" }
  ]
}
